<%-include("header") -%>

<div class="outer-container-allusers">
    <div class="main-container">

        <h2>List Of All The Residents:</h2>

        <table class="table table-dark table-hover table-striped table-bordered users-table tableBodyScroll ">
            <thead class="bg-info">
                <tr>
                    <th>Flat No.</th>
                    <th>Name</th>
                    
                    <th>Flat Alloc Date</th>
                    <th>Rent Details</th>
                    <th>Bill Details</th>
                    <!--  <TH>Pending Approvals</TH>-->
                </tr>
            </thead>
            <tbody class="allusers-table">
                <% receivedData.forEach(function(dataItem){ %>
                <tr>
                    <td><%= dataItem.flatId %></td>
                    <td> <%= dataItem.name %><i class="fa fa-phone btn btn-sm btn-light edit-user" data-toggle="tooltip" data-placement="right" title="<%= dataItem.contact %>"></i>
                      <a href="delete/<%=  dataItem.contact %>"> <i  class="fas fa-trash btn btn-sm btn-danger del-user"></i></a> </td>
                           
                            <td><%= dataItem.startingDate %></td>  
                    <td><a href="tenantRent/<%=dataItem.flatId%>"><i class="fas fa-info-circle btn btn-sm btn-light"></i></a></td>
                    <td><a href="tenantBillFull/<%=dataItem.flatId%>"><i class="fas fa-info-circle btn btn-sm btn-light "></i></a></td>
                    </tr>
                        <% }); %>

            </tbody>
        </table>

        <a class="btn btn-md btn-outline-warning add-user" href="tenantAddTable"> Add New Tenant</a>
      

    </div>
</div>
<%-include("footer") -%>